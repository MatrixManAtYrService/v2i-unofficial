-- 
-- Begin module: EU
-- 
-- ^-^-^-^-^-^-^-^-^-^-^-^-^-^-^-^-^-^-^-^-^-^-^-^-^-^-^-^-
EU DEFINITIONS AUTOMATIC TAGS::= BEGIN 
 


-- DF_Altitude_EU (Desc Name) Record 1
-- From source: AMENDMENT Regional Data J2735 (EU)
Altitude ::=  SEQUENCE {
   value        AltitudeValue,
   confidence   AltitudeConfidence
   }

-- DE_AltitudeConfidence_EU (Desc Name) Record 2
-- From source: AMENDMENT Regional Data J2735 (EU)
AltitudeConfidence ::= ENUMERATED {
   alt-000-01,  -- accuracy within    0.01 meter
   alt-000-02,  -- accuracy within    0.02 meter
   alt-000-05,  -- accuracy within    0.05 meter
   alt-000-10,  -- accuracy within    0.10 meter
   alt-000-20,  -- accuracy within    0.20 meter
   alt-000-50,  -- accuracy within    0.50 meter
   alt-001-00,  -- accuracy within    1.00 meter
   alt-002-00,  -- accuracy within    2.00 meter
   alt-005-00,  -- accuracy within    5.00 meter
   alt-010-00,  -- accuracy within   10.00 meter
   alt-020-00,  -- accuracy within   20.00 meter
   alt-050-00,  -- accuracy within   50.00 meter
   alt-100-00,  -- accuracy within  100.00 meter
   alt-200-00,  -- accuracy within  200.00 meter
   outOfRange,  -- accuracy exceeds 200.00 meters
   unavailable  -- unavailable
}

-- DE_AltitudeValue_EU (Desc Name) Record 3
-- From source: AMENDMENT Regional Data J2735 (EU)
AltitudeValue ::= INTEGER (-100000..800001)  -- units of  0.01 meter 
   -- Where: 
   -- seaLevel(0), 
   -- oneCentimeter(1), 
   -- unavailable(800001)

-- DE_EmissionType_EU (Desc Name) Record 10
-- From source: AMENDMENT Regional Data J2735 (EU)
EmissionType ::= ENUMERATED {
     typeA,  -- check for proper restrictions
     typeB,  -- 
     typeC,  -- 
     typeD,  -- 
     typeE,  -- 
     ... -- # LOCAL_CONTENT
     }

-- DF_PrioritizationResponse_EU (Desc Name) Record 34
-- From source: AMENDMENT Regional Data J2735 (EU)
PrioritizationResponse ::=  SEQUENCE {
   stationID      DSRC.StationID,
                  -- Id of requesting vehicle
                  -- Note that the stationID has to remain unchanged 
                  -- during the whole prioritisation process
   priorState     DSRC.PrioritizationResponseStatus,
                  -- State of prioritization request
   signalGroup    DSRC.SignalGroupID,
                  -- id of prioritized LaneSet, which will 
                  -- be given free way 
   ... -- # LOCAL_CONTENT
   }

-- DF_PrioritizationResponseList_EU (Desc Name) Record 35
-- From source: AMENDMENT Regional Data J2735 (EU)
PrioritizationResponseList ::=  SEQUENCE SIZE(1..10) OF PrioritizationResponse

-- DF_Reg-ConnectionManeuverAssist_EU (Desc Name) Record 38
-- From source: AMENDMENT Regional Data J2735 (EU)
EU-Reg-ConnectionManeuverAssist ::=  SEQUENCE {
    vehicleToLanePositions  VehicleToLanePositionList,
    rsuDistanceFromAnchor   DSRC.NodeOffsetPoint OPTIONAL
}

-- DF_REG_IntersectionState_EU (Desc Name) Record 42
-- From source: AMENDMENT Regional Data J2735 (EU)
EU-Reg-IntersectionState ::=  SEQUENCE { 
   activePrioritizations PrioritizationResponseList OPTIONAL,
   ... }

-- DF_REG_MapData_Base_EU   NEW (Desc Name) Record 48
-- From source: AMENDMENT Regional Data J2735 (EU)
EU-Reg-MapData ::=  SEQUENCE { 
   signalHeadLocations   SignalHeadLocationList OPTIONAL,
   ... 
   }

-- DF_REG_Position3D_EU (Desc Name) Record 57
-- From source: AMENDMENT Regional Data J2735 (EU)
EU-Reg-Position3D ::=  SEQUENCE {
   altitude    Altitude, 
   ...
   }

-- DF_REG_RestrictionUserType_EU (Desc Name) Record 58
-- From source: AMENDMENT Regional Data J2735 (EU)
EU-Reg-RestrictionUserType ::=  SEQUENCE {
   emission   EmissionType OPTIONAL,
   ...
   }

-- DF_SignalHeadLocation_EU    NEW (Desc Name) Record 66
-- From source: AMENDMENT Regional Data J2735 (EU)
SignalHeadLocation ::=  SEQUENCE {
   node             DSRC.NodeOffsetPoint, -- the location
   signalGroupID    DSRC.SignalGroupID,   
   ...
}

-- DF_SignalHeadLocationList_EU   NEW (Desc Name) Record 67
-- From source: AMENDMENT Regional Data J2735 (EU)
SignalHeadLocationList ::=  SEQUENCE (SIZE(1..20)) OF  SignalHeadLocation

-- DF_VehicleToLanePosition_EU (Desc Name) Record 73
-- From source: AMENDMENT Regional Data J2735 (EU)
VehicleToLanePosition ::=  SEQUENCE {
    stationID   DSRC.StationID, 
    laneID      DSRC.LaneID,
    ... -- # LOCAL_CONTENT
    }

-- DF_VehicleToLanePositionList_EU (Desc Name) Record 74
-- From source: AMENDMENT Regional Data J2735 (EU)
VehicleToLanePositionList ::=  SEQUENCE SIZE(1..5) OF VehicleToLanePosition


